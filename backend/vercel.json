{

  "version": 2,
  "builds": [
    {
      "src": "api/index.py",

  "builds": [
    {
      "src": "app/main.py",

      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",

      "dest": "api/index.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.11"
  }
},
      "dest": "app/main.py"
    
  ]
}
